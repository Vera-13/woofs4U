<h1>Review your experience</h1>
<div class="container_rating">
  <%= simple_form_for [@dog, @review] do |f| %>
  <%= f.input :rating, as: :select, collection: 1..5 %>
  <%= f.input :comment %>
  <%= f.submit "Review", class: 'btn' %>
  <%= link_to "Back", dog_path(@dog), class: 'btn' %>
  <% end %>
</div>
